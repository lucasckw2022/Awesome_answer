<%# if user sign in and liked this question%>
<% if user_signed_in? && user_like %>
<%# pass it to nested like by passing the @question and like object%>
  <%= link_to "Un-Like", question_like_path(@question, user_like), remote: true, method: :delete %>
<% else %>
  <%= link_to "Like", question_likes_path(@question), remote: true, method: :post %>
<% end%>
(<%= @question.likes.count%>)
